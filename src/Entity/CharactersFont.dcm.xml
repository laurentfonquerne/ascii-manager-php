      <generator strategy="IDENTITY"/>
    </id>
    <field name="fontId" type="integer" column="font_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="charactersId" type="integer" column="characters_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="charactersFontWidth" type="integer" column="characters_font_width" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
